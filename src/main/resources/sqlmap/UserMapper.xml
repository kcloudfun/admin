<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.likai.admin.dao.IUserDao" >

	<select id="selectAll" resultType="com.likai.admin.po.User">
		SELECT
			id,
			account,
			password,
			name,
			sex
		FROM t_user
	</select>

	<insert id="InsertUser" parameterType="com.likai.admin.po.User">
		INSERT INTO 
			t_user 
		VALUES 
			(
				default,
				#{account},
				#{password},
				#{name},
				#{sex}
			)
	</insert>
	
</mapper>